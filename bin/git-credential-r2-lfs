#!/usr/bin/env sh
set -eu

case "$1" in
get)
	# consume stdin (git sends host/protocol info)
	while IFS='=' read -r key value; do [ -z "$key" ] && break; done
	echo "username=$(op read 'op://Personal/Cloudflare/e52zphvlox2hxnljodiodfucba/Access Key ID')"
	echo "password=$(op read 'op://Personal/Cloudflare/e52zphvlox2hxnljodiodfucba/Secret Access Key')"
	;;
esac
