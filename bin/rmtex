#!/usr/bin/env bash
set -euo pipefail

# clears misc files generated by latexmk
find "${@:-.}" -type f -name "*.aux" -delete
find "${@:-.}" -type f -name "*.fdb_latexmk" -delete
find "${@:-.}" -type f -name "*.fls" -delete
find "${@:-.}" -type f -name "*.log" -delete
find "${@:-.}" -type f -name "*.synctex.gz" -delete
